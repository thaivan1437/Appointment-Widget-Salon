{
  "name": "belmont-beauty-salon",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run package:prepare && node inject-widget.js --buildEnv=development && npm run dev",
    "package:prepare": "npm install && npm run clean",
    "clean": "rm -rf build",
    "dev": "webpack-dev-server",
    "build:dev": "webpack -p --buildEnv=development",
    "build:prod": "webpack -p --buildEnv=production",
    "dist:dev": "npm run package:prepare && npm run build:dev && node inject-widget.js --buildEnv=development",
    "dist:prod": "npm run package:prepare && npm run build:prod && node inject-gtm.js && node inject-widget.js --buildEnv=production",
    "local": "PUBLIC_NEXT_ENV=local next dev",
    "next-dev": "PUBLIC_NEXT_ENV=development next dev",
    "next-build": "PUBLIC_NEXT_ENV=production && next build",
    "next-start": "PUBLIC_NEXT_ENV=production && next start",
    "build-sitemap": "node scripts/sitemap-common.js --buildEnv=production",
    "build-sitemap:dev": "node scripts/sitemap-common.js",
    "build:ci": "PUBLIC_NEXT_ENV=production next build && npm run build-sitemap",
    "export": "PUBLIC_NEXT_ENV=production next export",
    "build-dev:ci": "PUBLIC_NEXT_ENV=development next build && npm run build-sitemap:dev",
    "export-dev": "PUBLIC_NEXT_ENV=development next export",  
    "nodemon": "nodemon -e js,json,css,scss,jsx",
    "start:local": "export PUBLIC_NEXT_ENV=development && next build && next export && serve out"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  },
  "dependencies": {
    "@sentry/react": "^5.27.6",
    "@sentry/tracing": "^5.27.6",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "bootstrap": "^4.5.3",
    "globby": "^11.0.1",
    "history": "^4.10.1",
    "mini-css-extract-plugin": "^0.9.0",
    "next": "^10.1.2",
    "node-sass": "^4.13.0",
    "prettier": "^2.2.1",
    "react": "^16.14.0",
    "react-animated-css": "^1.2.1",
    "react-animated-slider": "^1.1.4",
    "react-app-polyfill": "^1.0.6",
    "react-bootstrap": "^1.4.0",
    "react-device-detect": "^1.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "serve": "^11.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/node": "^7.12.6",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "babel-loader": "^8.2.1",
    "css-loader": "^3.6.0",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "nodemon": "^2.0.7",
    "react-hot-loader": "^4.13.0",
    "replace-in-file": "^4.3.1",
    "style-loader": "^1.3.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-filter-warnings-plugin": "^1.2.1",
    "yargs": "^14.2.3"
  },
  "peerDependencies": {
    "postcss": "^8.0.0"
  }
}
