{
  "name": "salon-manager-widgets",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run package:prepare && npm run dev",
    "package:prepare": "npm install && npm run clean",
    "clean": "rm -rf build",
    "dev": "webpack-dev-server --buildEnv=local",
    "build": "webpack -p",
    "dist:dev": "npm run package:prepare && npm run build -- --buildEnv=development",
    "dist:prod": "npm run package:prepare && npm run build -- --buildEnv=production",
    "prettier:fix": "prettier --write src/**/*.{js,jsx}"
  },
  "dependencies": {
    "@sentry/browser": "^5.19.0",
    "axios": "^0.19.2",
    "lodash.sortby": "^4.7.0",
    "react": "^16.13.1",
    "react-datepicker": "^2.16.0",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-responsive": "^8.1.0",
    "react-slick": "^0.25.2",
    "rodal": "^1.8.1",
    "slick-carousel": "^1.8.1",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react-app": "^9.1.2",
    "css-loader": "^3.6.0",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.19.1",
    "style-loader": "^1.2.1",
    "url-loader": "^2.3.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "yargs": "^14.2.3"
  }
}
