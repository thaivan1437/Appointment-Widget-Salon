{
  "name": "salon-manager-widgets",
  "version": "1.0.0",
  "description": "Salon Manager Widgets",
  "scripts": {
    "start": "npm run package:prepare && npm run local",
    "package:prepare": "npm install && npm run clean",
    "clean": "del -rf build",
    "build": "webpack -p",
    "local": "webpack-dev-server --buildEnv=local",
    "dist:dev": "npm run package:prepare && npm run build -- --buildEnv=development",
    "dist:prod": "npm run package:prepare && npm run build -- --buildEnv=production",
    "prettier:fix": "prettier --write src/**/*.{js,jsx}"
  },
  "dependencies": {
    "@sentry/browser": "^5.22.3",
    "@sentry/react": "^5.22.3",
    "@sentry/tracing": "^5.22.3",
    "axios": "^0.20.0",
    "lodash.find": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "react": "^16.13.1",
    "react-datepicker": "^3.1.3",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-responsive": "^8.1.0",
    "react-slick": "^0.27.10",
    "rodal": "^1.8.1",
    "slick-carousel": "^1.8.1",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react-app": "^9.1.2",
    "css-loader": "^4.2.2",
    "html-webpack-plugin": "^4.4.1",
    "prettier": "^2.1.1",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "yargs": "^15.4.1"
  }
}
